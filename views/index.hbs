<h1>{{title}}</h1>
<p>Welcome to {{title}}</p>

{{#if username}}
    <p>User : {{id}} logged in.</p>
{{else}}
    <p>Click <a href="/login">here to login</a></p>
{{/if}}

<ul class="department_list">
    {{#each results}}
        <li>{{this.name}}</li>
        <li>{{this.like}}</li>
    {{/each}}
</ul>